{
    "name": "spryker/stores-rest-api",
    "type": "library",
    "description": "StoresRestApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/application-extension": "^1.1.0",
        "spryker/country": "^3.2.0 || ^4.0.0",
        "spryker/currency": "^3.2.0 || ^4.0.0",
        "spryker/glue-application": "^1.0.0",
        "spryker/glue-application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/store": "^1.8.0",
        "spryker/symfony": "^3.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*"
    },
    "suggest": {
        "spryker/container": "Add this module when you want to use the Container."
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
